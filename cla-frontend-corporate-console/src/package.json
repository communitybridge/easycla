{
  "name": "cla-frontend-corporate-console",
  "license": "MIT",
  "author": "The Linux Foundation",
  "private": true,
  "os": [
    "linux",
    "darwin"
  ],
  "scripts": {
    "clean": "./node_modules/.bin/ionic-app-scripts clean",
    "build": "./node_modules/.bin/ionic-app-scripts build",
    "prebuild:dev": "STAGE_ENV=dev node ./config/scripts/prefetch-ssm.js",
    "build:dev": "STAGE_ENV=dev ./node_modules/.bin/ionic-app-scripts build",
    "serve:dev": "USE_LOCAL_SERVICES=false STAGE_ENV=dev ./node_modules/.bin/ionic-app-scripts serve -p 8101 -r 8102 --dev-logger-port 8103",
    "serve:dev:local": "USE_LOCAL_SERVICES=true STAGE_ENV=dev ./node_modules/.bin/ionic-app-scripts serve -p 8101 -r 8102 --dev-logger-port 8103",
    "prebuild:staging": "STAGE_ENV=staging node ./config/scripts/prefetch-ssm.js",
    "build:staging": "STAGE_ENV=staging ./node_modules/.bin/ionic-app-scripts build",
    "serve:staging": "STAGE_ENV=staging ./node_modules/.bin/ionic-app-scripts serve --prod",
    "prebuild:prod": "STAGE_ENV=prod node ./config/scripts/prefetch-ssm.js",
    "build:prod": "STAGE_ENV=prod ./node_modules/.bin/ionic-app-scripts build",
    "serve:prod": "STAGE_ENV=prod ./node_modules/.bin/ionic-app-scripts serve --prod"
  },
  "dependencies": {
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@auth0/auth0-spa-js": "^1.12.1",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "2.1.3",
    "@swimlane/ngx-datatable": "^11.3.2",
    "@types/lodash": "4.14.112",
    "@types/node": "^8.0.17",
    "aws-sdk": "^2.304.0",
    "chart.js": "^2.5.0",
    "google-libphonenumber": "^2.0.18",
    "graceful-fs": "^4.2.2",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "jwt-decode": "2.2.0",
    "ngx-pagination": "^4.1.0",
    "query-string": "^6.13.6",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "timsort": "^0.3.0",
    "url-parse": "^1.5.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.2.0",
    "typescript": "3.0.1"
  },
  "cordovaPlugins": [
    "cordova-plugin-whitelist",
    "cordova-plugin-statusbar",
    "cordova-plugin-console",
    "cordova-plugin-splashscreen",
    "cordova-plugin-device",
    "ionic-plugin-keyboard"
  ],
  "resolutions": {
    "bl": "^1.2.3",
    "cryptiles": "^4.1.2",
    "hoek": "^4.2.1",
    "ini": "^1.3.7",
    "kind-of": "^6.0.3",
    "mem": "^4.0.0",
    "minimist": "^1.2.3",
    "node-sass": "4.13.1",
    "tunnel-agent": "^0.6.0",
    "yargs-parser": "13.1.2"
  },
  "cordovaPlatforms": [
    "ios",
    {
      "platform": "ios",
      "version": "",
      "locator": "ios"
    }
  ],
  "config": {
    "ionic_purge_decorators": false,
    "ionic_src_dir": "ionic"
  },
  "description": "cla-corporate-console"
}
